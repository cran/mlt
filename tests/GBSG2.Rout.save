> bx <- as.basis(as.formula(paste("~", paste(xvar, collapse = "+"))), data = GBSG2,
+                remove_intercept = TRUE)
> 
> m <- ctm(by, shift = bx, todist = "MinEx")
> 
> mod <- mlt(m, data = GBSG2, scale = TRUE, check = FALSE)
